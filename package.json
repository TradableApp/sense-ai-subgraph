{
  "name": "sense-ai",
  "license": "UNLICENSED",
  "scripts": {
    "// --- GENERATION ---": "---",
    "prepare:localnet": "mustache config/localnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:testnet": "mustache config/base-testnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mainnet": "mustache config/base-mainnet.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "// --- LOCAL DEPLOYMENT ---": "---",
    "create-local": "graph create --node http://localhost:8020/ sense-ai",
    "remove-local": "graph remove --node http://localhost:8020/ sense-ai",
    "deploy-local": "npm run prepare:local && npm run codegen && npm run build && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 sense-ai",
    "// --- STUDIO DEPLOYMENT ---": "---",
    "deploy:testnet": "npm run prepare:testnet && npm run codegen && npm run build && graph deploy sense-ai-testnet",
    "deploy:mainnet": "npm run prepare:mainnet && npm run codegen && npm run build && graph deploy sense-ai",
    "// --- TESTING ---": "---",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.98.1",
    "@graphprotocol/graph-ts": "0.37.0"
  },
  "devDependencies": {
    "matchstick-as": "0.6.0"
  }
}
